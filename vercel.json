
{
  "version": 2,
  "builds": [
    {
      "src": "api/[...route].ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...route].ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "functions": {
    "api/[...route].ts": {
      "maxDuration": 30
    }
  }
}
